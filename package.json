{
  "name": "midnight-streak-online",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "dev": "npm run dev --workspace client",
    "server": "npm run dev --workspace server",
    "client": "npm run dev --workspace client",
    "dev:all": "concurrently -n SERVER,CLIENT -c auto \"npm run dev --workspace server\" \"npm run dev --workspace client\""
  },
  "workspaces": [
    "client",
    "server"
  ],
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
